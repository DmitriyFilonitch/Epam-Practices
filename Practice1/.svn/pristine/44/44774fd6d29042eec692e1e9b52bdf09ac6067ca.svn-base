package ua.nure.filonitch.practice1;

public class Part4 {
	public static void main(String[] args) {
		int a1 = Integer.parseInt(args[0]);
		int a2 = Integer.parseInt(args[1]);
		int d = 0;
		while (a1 != 0 && a2 != 0) {
			if (a1 > a2) {
				a1 %= a2;
			} else {
				a2 %= a1;
			}
			d = a1 + a2;
		}
		System.out.println(d);
	}
}
